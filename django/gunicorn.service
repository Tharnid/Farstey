[Unit]

Description=gunicorn daemon
Requires=gunicorn.socket
After=network.target

[Service]
User=tharnid
Group=www-data
WorkingDirectory=/home/tharnid/stabrabbits/stabrabbits
ExecStart=/home/tharnid/stabrabbits/srenv/bin/gunicorn \
          --access-logfile - \
          --workers 3 \
          --bind unix:/run/gunicorn.sock \
          stabrabbits.wsgi:application


[Install]
WantedBy=multi-user.target
